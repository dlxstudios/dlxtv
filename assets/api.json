{
  "title": "dlxTV",
  "routes": [
    {
      "title": "Movies",
      "path": "/home",
      "components": [
        {
          "type": "Appbar",
          "Appbar": {
            "title": "Dope Movies",
            "backgroundColor": "green"
          }
        },
        {
          "type": "MediaGrid",
          "MediaGrid": {
            "gridRatio": "0.6",
            "itemsUrl": "http://api.themoviedb.org/3/movie/now_playing?api_key=130979ed329ff88f27bae32b141082fb",
            "itemsSelector": "results",
            "poster": "https://image.tmdb.org/t/p/original{poster_path}",
            "title": "{title}"
          }
        },
        {
          "type": "MediaGrid",
          "MediaGrid": {
            "gridRatio": "01",
            "itemsUrl": "https://api.soundcloud.com/tracks?client_id=7ce7c8ce794f080121b96a32aeff64f4",
            "poster": "{artwork_url}",
            "title": "{title}"
          }
        }
      ]
    },
    {
      "title": "SCloud",
      "path": "/homeSC",
      "type": "MediaGrid",
      "MediaGrid": {
        "gridRatio": "01",
        "itemsUrl": "https://api.soundcloud.com/tracks?client_id=7ce7c8ce794f080121b96a32aeff64f4",
        "poster": "{artwork_url}",
        "title": "{title}"
      }
    },
    {
      "title": "YTube",
      "path": "/home1",
      "components": [
        {
          "type": "List",
          "List": {
            "direction" : "horizontal",
            "itemsSelector": "items",
            "title": "{snippet.title}",
            "poster": "https://i.ytimg.com/vi/{snippet.resourceId.videoId}/maxresdefault.jpg",
            "gridRatio": "1",
            "itemsUrl": "https://www.googleapis.com/youtube/v3/playlistItems?key=AIzaSyDDBCXGpHKwKWiBBNSc-PN6tkuKj_3roPE&part=snippet%2CcontentDetails&maxResults=25&playlistId=PL19ohT5PbRowHisVkffh8TsyyqRHpJvN6"
          }
        }
      ]
    }
  ]
}